# Makefile for src/compat/

SHELL = @SHELL@
top_srcdir = @top_srcdir@
srcdir = @srcdir@
VPATH = @srcdir@

include $(top_srcdir)/build/build.mk

@SET_MAKE@

STRIP = @STRIP@
CXXFLAGS = @CXXFLAGS@ -I../.. -I$(top_srcdir) -I$(top_srcdir)/lib -I$(top_srcdir)/src @DEFS@ $(CFLGS)
CPPFLAGS = @CPPFLAGS@

OBJS = dirname.o \
	dn_expand.o \
	inet_ntop.o \
	snprintf.o \
	memcpy.o \
	memmem.o \
	memset.o \
	memutil.o \
	strcasecmp.o \
	strftime.o \
	strlcpy.o \
	strsep.o

doofus:
	@echo ""
	@echo "Let's try this from the right directory..."
	@echo ""
	@cd ../.. && $(MAKE)

clean:
	@rm -f .depend *.o *~

compat: $(OBJS)

include ./.deps/includes
